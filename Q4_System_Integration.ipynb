    "## City Reporter - News Categorization and Routing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "def load_saved_model():\n",
    "    \"\"\"Load trained Naive Bayes model from Q2.\"\"\"\n",
    "    return news_model, news_vectorizer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "def categorise_daily_news(model, vectorizer):\n",
    "    \"\"\"Read and categorise news from Daily_News.csv.\"\"\"\n",
    "    # Read the CSV file\n",
    "    news_df = pd.read_csv('Daily_News.csv', encoding = 'utf-8-sig')\n",
    "\n",
    "    # Combine headline and description for classification\n",
    "    news_df['combined'] = news_df['Summary'] + \" \" + news_df['Text']\n",
    "\n",
    "    # Vectorize and predict categories\n",
    "    X = vectorizer.transform(news_df['combined'])\n",
    "    news_df['predicted_category'] = model.predict(X)\n",
    "\n",
    "    return news_df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_matching_news(news_df, city, category):\n",
    "    \"\"\"Filter news for specific city and category.\"\"\"\n",
    "    matching = news_df[(news_df['Location'] == city) & (news_df['predicted_category'] == category)]\n",
    "\n",
    "    return matching"
   ]
  },
  {
