    "def citypulse_ai_system():\n",
    "    \"\"\"Main entry point for CityPulse AI.\"\"\"\n",
    "    while True:\n",
    "        choice = main_menu()\n",
    "\n",
    "        if choice == '1':\n",
    "            # Executive Correspondent\n",
    "            print(\"\\n\" + \"=\" * 60)\n",
    "            print(\"Executive Correspondent - Office Visit Planner\")\n",
    "            print(\"=\" * 60)\n",
    "\n",
    "            # Get offices to visit\n",
    "            print(\"\\nAvailable offices: Los Angeles, San Diego, Las Vegas\")\n",
    "            offices_input = input(\"Enter offices to visit: \")\n",
    "            offices = [city.strip() for city in offices_input.split(',')]\n",
    "\n",
    "            print(f\"\\nPlanning optimal route for: {offices}\")\n",
    "            print(\"Running Genetic Algorithm...\")\n",
    "\n",
    "            best_route, best_distance = genetic_algorithm_tsp(offices, office_distances,\n",
    "                                                              population_size = 50,\n",
    "                                                              generations = 100,\n",
    "                                                              mutation_rate = 0.02)\n",
    "\n",
    "            print(f\"\\nOptimal Route Found:\")\n",
    "            print(f\"Phoenix HQ ---> {' ---> '.join(best_route)} ---> Phoenix HQ\")\n",
    "            print(f\"Total Distance: {best_distance} miles\")\n",
    "\n",
    "        elif choice == '2':\n",
    "            # City Reporter\n",
    "            print(\"\\n\" + \"=\" * 60)\n",
    "            print(\"City Reporter - News Categorisation & Routing\")\n",
    "            print(\"=\" * 60)\n",
    "\n",
    "            # Load model\n",
    "            model, vectorizer = load_saved_model()\n",
    "            print(\"Model loaded\")\n",
    "\n",
    "            # Categorise news\n",
    "            print(\"Categorising daily news...\")\n",
    "            news_df = categorise_daily_news(model, vectorizer)\n",
    "\n",
    "            # Get user input\n",
    "            city = input(\"\\nEnter your city: \").strip()\n",
    "            category = input(\"Enter news category (Sports, Politics, etc.): \").strip().upper()\n",
    "\n",
    "            # Find matching news\n",
    "            matching = find_matching_news(news_df, city, category)\n",
    "\n",
    "            if len(matching) > 0:\n",
    "                print(f\"\\nFound {len(matching)} {category} article(s) in {city}:\")\n",
    "                for idx, row in matching.iterrows():\n",
    "                    print(f\"\\n{row['Summary']}\")\n",
    "                    print(f\"Location: {row['Location']}\")\n",
    "                    print(f\"Category: {row['predicted_category']}\")\n",
    "            else:\n",
    "                print(f\"\\nNo {category} news found in {city}\")\n",
    "\n",
    "        elif choice == '3':\n",
    "            print(\"\\nExiting CityPulse AI\")\n",
    "            break"
   ]
